<?xml version="1.0" encoding="utf-8" ?>
<parameters>
  <parameter name="Environment" description="Variable to show values being updated on deployment">
    <parameterEntry kind="XmlFile" scope="\\Web.config$" match="/configuration/appSettings/add[@key='Environment']/@value" />
  </parameter>
  <parameter name="SecretValue" description="Variable to example setting">
    <parameterEntry kind="XmlFile" scope="\\Web.config$" match="/configuration/appSettings/add[@key='SecretValue']/@value" />
  </parameter>
</parameters>